{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb",
      "prettier",
      "jest",
      "json/recommended",
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier",
      "jest",
      "json/recommended"
  ],
  "rules": {
      "prettier/prettier": ["warn", {"endOfLine": "auto"}],
      "no-console":"off",
      "react/prop-types":"off",
      "react/jsx-filename-extension":"off",
      "react/function-component-definition": ["warn", {
          "namedComponents": "arrow-function",
          "unnamedComponents":
          "arrow-function"
        }]
  }
}
